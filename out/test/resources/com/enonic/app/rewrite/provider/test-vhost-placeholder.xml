<?xml version="1.0" encoding="UTF-8" ?>
<urlrewrite>
  <rule match-type="wildcard">
    <from>{{vhost:myvhost}}/ost/fisk/*</from>
    <to type="redirect">/en/$1</to>
  </rule>
  <rule match-type="wildcard">
    <from>{{vhost:myvhost}}/ost/tjommi</from>
    <to type="redirect">/pung</to>
  </rule>
  <rule match-type="wildcard">
    <from>{{vhost:myvhost}}/polsefjes</from>
    <to type="redirect">/nigirisushi</to>
  </rule>
</urlrewrite>